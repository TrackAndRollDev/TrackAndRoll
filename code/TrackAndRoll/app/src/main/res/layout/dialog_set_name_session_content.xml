<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:clickable="true"
    android:gravity="center"
    >

    <EditText
        android:id="@+id/etSetSessionName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dialog_text_margin"
        android:background="@drawable/rect_border_grey"
        android:hint="@string/dialog_set_name_session_et_hint"
        android:padding="@dimen/dialog_set_name_session_et_padding"
        android:textSize="@dimen/dialog_set_name_session_et_text_size"
        android:textColorHint="@color/colorTextHint"
        />


</LinearLayout>